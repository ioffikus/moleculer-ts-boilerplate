{
  "name": "kernel-services",
  "version": "0.0.3",
  "description": "all kernel microservices in this repo",
  "scripts": {
    "build": "webpack",
    "dev": "sh ./scripts/dev.sh",
    "start:hot": "npx moleculer-runner ./dist --repl --config ./dist/moleculer.config.js --hot",
    "start": "npx moleculer-runner ./dist --repl --config ./dist/moleculer.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint --ext .ts . --fix",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --testTimeout=30000 --runInBand --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:cov": "npm run test -- --coverage --verbose --forceExit",
    "test:changed": "npm run test -- --onlyChanged --forceExit",
    "test:ci": "npm run test -- --ci --reporters=default --coverage --forceExit --passWithNoTests",
    "tsoa:gen": "npx tsoa swagger -c ./src/nodes/rest-gateway/tsoa.json && npx tsoa routes -c ./src/nodes/rest-gateway/tsoa.json",
    "docker:stop-all": "sh ./scripts/docker-stop-all.sh",
    "docker:prune": "sh ./scripts/docker-prune.sh",
    "docker:rm-all": "sh ./scripts/docker-rmi-all.sh",
    "dc:dev": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml run --service-ports app",
    "dc:test": "docker-compose -f docker-compose.yaml -f docker-compose.test.yaml up --abort-on-container-exit app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run dc:test",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bluebird": "3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-locale": "^2.0.0",
    "express-useragent": "^1.0.13",
    "fast-json-parse": "^1.0.3",
    "fast-json-stringify": "^1.16.0",
    "fast-xml-parser": "^3.15.1",
    "got": "^10.2.1",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.19",
    "luxon": "^1.21.3",
    "md5": "^2.2.1",
    "method-override": "^3.0.0",
    "mockdate": "^2.0.5",
    "moleculer": "0.13.12",
    "moleculer-cron": "0.0.2",
    "moleculer-db": "^0.8.4",
    "moleculer-db-adapter-mongoose": "^0.8.3",
    "moleculer-decorators": "^1.1.1",
    "moleculer-prometheus": "0.2.4",
    "nanoid": "2.1.8",
    "pino": "5.15.0",
    "pump": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "webpack": "^4.41.5",
    "webpack-merge": "^4.2.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.14.3",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "8.3.5",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.5.38",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.1.4",
    "@types/pino": "^5.15.1",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/luxon": "^1.21.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "6.0.3",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "23.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "moleculer-repl": "^0.6.2",
    "nock": "^11.7.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "3.5.0",
    "prettier": "^1.19.1",
    "swagger-ui-express": "^4.1.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tsoa": "^2.5.11",
    "typescript": "^3.7.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "author": "ioffikus",
  "license": "ISC"
}
